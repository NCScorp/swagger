{
    "name": "nasajon/atendimento",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Nasajon\\Atendimento\\AppBundle\\": "src/Nasajon/Atendimento/AppBundle", 
            "Nasajon\\MDABundle\\": "src/Nasajon/MDABundle"
        },
        "classmap": ["app/AppKernel.php", "app/AppCache.php"]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "^3.3",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "~1.6",
        "symfony/assetic-bundle": "~2.3",        
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.0.0",
        "twig/extensions": "~1.3",
        "braincrafted/bootstrap-bundle": "~2.2",
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "friendsofsymfony/rest-bundle": "^2.3",
        "lightsaml/sp-bundle": "^1.1",
        "nasajon/login-bundle": "dev-master",
        "nasajon/model-bundle": "dev-master",
        "nasajon/componentes-interface-bundle": "dev-mdauiselect-removeFilter",
        "nasajon/sdk" : "dev-master",
        "jms/serializer-bundle": "^1.1",
        "knplabs/knp-gaufrette-bundle": "~0.3",
        "oneup/uploader-bundle": "~1.4",
        "aws/aws-sdk-php-symfony": "~1.0",
        "exercise/htmlpurifier-bundle": "^0.2.2",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "mailgun/mailgun-php": "~2.0",
        "neitanod/forceutf8": "^2.0",
        "kriansa/openboleto": "dev-master",
        "sentry/sentry-symfony": "^1.0",
        "firebase/php-jwt": "^4.0",
        "aws/aws-php-sns-message-validator": "^1.4",
        "php-amqplib/rabbitmq-bundle": "1.14.4",
        "php-amqplib/php-amqplib": "2.7.2",
        "stevenmaguire/oauth2-keycloak": "^2.1",        
        "guzzlehttp/guzzle": "^6.0",
        "ekreative/health-check-bundle": "^1.4",
        "maxbanton/cwh": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "phpunit/phpunit": "4.8.*",
        "nasajon/mdatransformer": "dev-v2.0-alpha-ajuste",
        "nasajon/bancosweb": "dev-master",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "codeception/codeception" : "^2.3"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [            
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1"
        },
        "process-timeout": 3600
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Nasajon/LoginBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/ModelBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/ComponentesInterfaceBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/SDK"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/MdaPhpTransformer"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/php-itaucripto"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/openboleto"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/BancosWeb"
        }

    ]
}
