{
    "compilerOptions":{
       "target":"es5",
       "sourceMap":true,
       "noImplicitAny":false,
       "skipLibCheck":true,
       "resolveJsonModule":true,
       "esModuleInterop":true,
       "downlevelIteration":true,
       "experimentalDecorators":true,
       "moduleResolution":"node",
       "lib":[
          "es5",
          "es6",
          "dom",
          "esnext"
       ],
       "baseUrl":".",
       "paths":{
            "@nsj/core":[
                "assets/ts/ns-core"
            ],
            "@nsj/core/*":[
             "assets/ts/ns-core/*"
          ]
       }
    },
    "include":[
       "assets",
       "config",
       "assets/ts/modulos"
    ]
}
