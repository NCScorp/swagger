                        <div class="panel-body">

                            {% block index_list -%}

                                <div class="list-group" id="atendimento_cliente_solicitacoes_list" >
                                    {%- block viewedit_view_list_item %}
                                    <a href ui-sref='atendimento_cliente_solicitacoes_show({atendimento : entity.atendimento})'  class="list-group-item" ng-repeat="entity in tndmnt_clnt_slctcs_lst_cntrllr.entities" ui-sref-active="active">
                                        <div class="status pull-right">
                                            <span class="info-post" am-time-ago="entity.data_ultima_resposta | asDate"></span>
                                        </div>
                                        <h4 class="list-group-item-heading">#{{'{{ entity.numeroprotocolo }} '}}</h4>
                                        <p class="list-group-item-text" ng-bind-html="entity.resumo | highlight : tndmnt_clnt_slctcs_lst_cntrllr.filter.key"></p>
                                        <p class="list-group-item-text ultima-resposta" ng-if="entity.ultima_resposta_admin"><span class="reply-ico"><i class="el el-share-alt"></i></span><span ng-bind-html="entity.ultima_resposta_resumo"></span></p>
                                        <span class="label label-default" ng-if="entity.cliente.cliente && tndmnt_clnt_slctcs_lst_cntrllr.usuarioLogado.getClientes().length > 1">{{'{{ tndmnt_clnt_slctcs_lst_cntrllr.usuarioLogado.getCliente(entity.cliente.cliente).cliente_nome }} '}}</span>
                                    </a>
                                    {%- endblock viewedit_view_list_item %}
                                    <p class="list-group-item" ng-if="!tndmnt_clnt_slctcs_lst_cntrllr.finished && (tndmnt_clnt_slctcs_lst_cntrllr.service.to_load <= 0)">
                                        <a href="" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.loadMore()">Ver mais...</a>
                                    </p>
                                    <p class="text-center article-loading height-full" ng-class="{'onloading' : tndmnt_clnt_slctcs_lst_cntrllr.isBusy() }" ng-if="tndmnt_clnt_slctcs_lst_cntrllr.isBusy()"></p>
                                    <p class="text-center nfound" ng-if="tndmnt_clnt_slctcs_lst_cntrllr.entities.length == 0 && !tndmnt_clnt_slctcs_lst_cntrllr.isBusy()">Nenhum dado encontrado</p>
                                </div>

                            {% endblock index_list -%}
                        </div><!--/panel-body-->
                    </div><!--/panel-->
                </div>
            </div>
        </div>
    {% endblock %}

    {% block index_builltin_form -%}
        <div class='col-md-{{12 - index_layout_size}} article-area height-full-calc row' ui-view>
            <div class="bt-new-article new-article message text-center height-full">
                <div class="initial-msg hidden-sm hidden-xs">
                    <i class="el el-inbox"></i>
                    <div ng-if="tndmnt_dmn_slctcs_lst_cntrllr.entities.length != 0">
                        <h2>Visualize ou Crie um Chamado.</h2>
