                                                                        <ui-select-match allow-clear="true" placeholder="Selecione ou busque">{{'{{atribuidoa.opt}}'}}</ui-select-match>
                                                                        <ui-select-choices group-by="tndmnt_clnt_slctcs_lst_cntrllr.agruparPorTipo" 
                                                                        repeat="option in tndmnt_clnt_slctcs_lst_cntrllr.contas | filter:$select.search">
                                                                            <div class="row">
                                                                                <div class="col-md-12" ng-bind-html="option? (option) : option | highlight: $select.search"></div>
                                                                            </div>
                                                                        </ui-select-choices>
