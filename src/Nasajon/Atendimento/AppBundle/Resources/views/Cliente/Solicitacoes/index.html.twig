                                                                <div>
                                                                    <select
                                                                        class="input-sm form-control"
                                                                        ng-model='adiado.opt'
                                                                        ng-change='tndmnt_clnt_slctcs_lst_cntrllr.reloadData()'>
                                                                        <option value="" disabled="disabled">Selecione</option>
                                                                        <option value="true">Sim</option>
                                                                        <option value="false">Não</option>
                                                                    </select>
                                                                </div>
                                                            </td>
                                                            <td width="5%" class="text-left">
                                                                <a href  ng-click="tndmnt_clnt_slctcs_lst_cntrllr.removeFiltro(adiado);" class="btn btn-danger btn-small btns-acoes"><i class="el el-remove"></i></a>
                                                            </td>
                                                        </tr>

                                                    </tbody>

                                                    <tbody ng-if="tndmnt_clnt_slctcs_lst_cntrllr.filtros[cc.atendimentocampocustomizado].length > 0" ng-repeat="cc in tndmnt_clnt_slctcs_lst_cntrllr.camposcustomizados">

                                                        <tr>
                                                            <td class="td-title" colspan="2">
                                                                <b>{{'{{cc.label}}'}}</b>
                                                            </td>
                                                        </tr>

                                                        <tr ng-repeat="campo in tndmnt_clnt_slctcs_lst_cntrllr.filtros[cc.atendimentocampocustomizado] track by $index">
