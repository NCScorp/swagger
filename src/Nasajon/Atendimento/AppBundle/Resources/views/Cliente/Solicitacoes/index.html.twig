{% embed '@NasajonMDABundle/Resources/js/Atendimento/Cliente/Solicitacoes/index.html.twig' %}

    {% block index_header -%}
        <div class="col-md-12 headline-page">
            <div class="headline clearfix">
                <h2 class="pull-left">Chamados</h2>
                <a ng-if="tndmnt_clnt_slctcs_lst_cntrllr.podeCriarChamado" class="btn btn-xs btn-warning" ui-sref="atendimento_cliente_solicitacoes_new({cliente : tndmnt_clnt_slctcs_lst_cntrllr.constructors.cliente,email : tndmnt_clnt_slctcs_lst_cntrllr.constructors.email})">
                    <i class="el el-plus"></i> Criar
                </a>
            </div>
        </div>
    {% endblock index_header -%}


    {% block index_layout %}
        <div class="height-full-calc hidden-print list-links col-md-4" ng-class="{ 'hidden-sm hidden-xs' : (('atendimento_cliente_solicitacoes_show'| isStateEq) || ('atendimento_cliente_solicitacoes_edit'| isStateEq) || ('atendimento_cliente_solicitacoes_new'| isStateEq ))}">
            {% set index_layout_size = 12 %}
            <div class="height-full col-md-{{index_layout_size}}">
                <div class='row height-full'>
                    <div class="panel panel-default">
                        {% block index_panel_header -%}
                            <div class="panel-heading clearfix">
                                {% block index_filter -%}
                                    <form id="list_filter">
                                        <div class="col-xs-8">
                                            <div>
                                                <input type="text" name="key" class="form-control campo-busca" ng-model="tndmnt_clnt_slctcs_lst_cntrllr.campoParaBusca" ng-change="tndmnt_clnt_slctcs_lst_cntrllr.search()" ng-model-options='{ debounce: 500 }'  placeholder="Buscar..." />
                                            </div><!-- /input-group -->
                                        </div>
                                        <div class="col-xs-4">
                                            {% block index_toolbar -%}
                                                <div class="btn-group pull-right" uib-dropdown role="group">
                                                    <button type="button" class="btn btn-default" title="Atualizar" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.recarregar()"><i class="el el-refresh"  aria-hidden="true"></i></button>
                                                    
                                                    <button ng-click="tndmnt_clnt_slctcs_lst_cntrllr.toggleFilter = !tndmnt_clnt_slctcs_lst_cntrllr.toggleFilter" type="button" class="btn btn-default" title="Filtros" ng-if="tndmnt_clnt_slctcs_lst_cntrllr.constructors.situacao != 2">
                                                        <i class="el el-filter"  aria-hidden="true"></i>
                                                    </button>

                                                    <button id="single-button" type="button" class="btn btn-default" uib-dropdown-toggle ng-disabled="disabled" ng-if="tndmnt_clnt_slctcs_lst_cntrllr.usuarioLogado.getClientes().length > 0">
                                                        <span class="icon xs-icon ret-icon"></span>
                                                    </button>
                                                    <ul class="dropdown-menu" uib-dropdown-menu role="menu" aria-labelledby="single-button">
                                                        <li role="menuitem">
                                                            <button class="btn-dropdown" ng-class="{active : tndmnt_clnt_slctcs_lst_cntrllr.menuSituacaoAtivo === 'todos'}" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filtrarSituacao('todos')">{{'todoschamados'|trans}}</button>
                                                        </li>
                                                         <li role="menuitem" ng-if="tndmnt_dmn_slctcs_lst_cntrllr.entity.usuarioLogado.USUARIO_SEM_CLIENTE_CRIAR_CHAMADO == 1">
                                                            <button class="btn-dropdown" ng-class="{active : tndmnt_clnt_slctcs_lst_cntrllr.menuSituacaoAtivo === 'meus'}" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filtrarSituacao('meus')">{{'meuschamados'|trans}}</button>
                                                        </li>
                                                        <li role="presentation" ui-sref-active="active"  ng-repeat="clnt in tndmnt_clnt_slctcs_lst_cntrllr.usuarioLogado.getClientes()" ng-if="tndmnt_clnt_slctcs_lst_cntrllr.usuarioLogado.getClientes().length > 1">
                                                            <button class="btn-dropdown" ng-class="{active : tndmnt_clnt_slctcs_lst_cntrllr.menuSituacaoAtivo === clnt.cliente_id}" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filtrarSituacao(clnt.cliente_id)">{{"{{clnt.cliente_nome}}"}}</button>
                                                        </li>
                                                        <li class="divider"></li>
                                                        <li class="dropdown-header">Ordenar por</li>
                                                        <li role="menuitem">
                                                            <button class="btn-dropdown" ng-class="{active : tndmnt_clnt_slctcs_lst_cntrllr.menuOrdenacaoAtiva === 'created_at_desc'}" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filterOrder('created_at_desc')">Mais recentes</button>
                                                        </li>
                                                        <li role="menuitem">
                                                            <button class="btn-dropdown" ng-class="{active : tndmnt_clnt_slctcs_lst_cntrllr.menuOrdenacaoAtiva === 'created_at_asc'}" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filterOrder('created_at_asc')">Mais antigas</button>
                                                        </li>
                                                        <li role="menuitem">
                                                            <button class="btn-dropdown" ng-class="{active : tndmnt_clnt_slctcs_lst_cntrllr.menuOrdenacaoAtiva === 'desc'}" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filterOrder('desc')">Resposta mais recentes</button>
                                                        </li>
                                                        <li role="presentation" ng-class="{'active': tndmnt_clnt_slctcs_lst_cntrllr.mim}" ng-if="tndmnt_clnt_slctcs_lst_cntrllr.usuarioLogado.usuarioSemClienteCriarChamado">
                                                            <a href ng-click="tndmnt_clnt_slctcs_lst_cntrllr.filtrarAtribuicao({ atribuidoa : 'mim', order: 'asc', adiado: false })"><span class="badge" ng-bind="tndmnt_clnt_slctcs_lst_cntrllr.abertos.mim"></span> {{'meuschamados'|trans}}</a>
                                                        </li>
                                                    </ul>
                                                </div>

                                            {% endblock index_toolbar -%}
                                        </div>

                                        <div class="col-sm-12">
                                            <div ng-show="tndmnt_clnt_slctcs_lst_cntrllr.toggleFilter" class="filter top-buffer">

                                                <table class="table no-border clear-bottom">
                                                    
                                                    <tbody ng-if="tndmnt_clnt_slctcs_lst_cntrllr.filtros.created_at_ini.length > 0 && tndmnt_clnt_slctcs_lst_cntrllr.filtros.created_at_fim.length > 0" class="row">
                                                     <tr>
                                                            <td class="td-title" colspan="2">
                                                                <b>Criado entre</b>
                                                            </td>
                                                        </tr>

                                                    <div class ="row">
                                                        <tr class="col-md-6" ng-repeat="created_at_ini in tndmnt_clnt_slctcs_lst_cntrllr.filtros.created_at_ini track by $index">
                                                            <td width="95%">
                                                                <div>
                                                                <div>
                                                                    <div class="input-group input-sm date" 
                                                                    id='datetimepicker6'/>
                                                                    <p class="input-group">
                                                                    <input type="text" 
                                                                    class="form-control input-sm" 
                                                                    uib-datepicker-popup="dd/MM/yyyy" 
                                                                    format="dd/MM/yyyy"
                                                                    ng-model="created_at_ini.opt"
                                                                    ng-model-options='{ debounce: 500 }'
                                                                    placeholder=""
                                                                    date-input=""
                                                                    name=""
                                                                    is-open="data.open" 
                                                                    datepicker-options="dateOptions" 
                                                                    ng-click="data.open=true"
                                                                    ng-required="true" current-text="Hoje" 
                                                                    clear-text="Limpar"
                                                                    close-text="Close"
                                                                    current-text='Hoje' 
                                                                    alt-input-formats="altInputFormats" 
                                                                    ng-change="tndmnt_clnt_slctcs_lst_cntrllr.checaFiltrosCreatedAt()" />
                                                                    <span class="input-group-btn">
                                                                    <button type="button" class="btn btn-default" ng-click="data.open=true"><i class="glyphicon glyphicon-calendar"></i></button>
                                                                    </span>
                                                                    </p>
                                                                </div>
                                                                </tr>
                                                        </div>
                                                                </td>
                                                             </td>
                                                        <tr class="col-md-6" ng-repeat="created_at_fim in tndmnt_clnt_slctcs_lst_cntrllr.filtros.created_at_fim track by $index">
                                                            <td width="95%">
                                                                <div>
                                                                <div>
                                                                    <div class="input-group input-sm date" 
                                                                    id='datetimepicker6'/>
                                                                    <p class="input-group">
                                                                    <input type="text" 
                                                                    class="form-control input-sm" 
                                                                    uib-datepicker-popup="dd/MM/yyyy" 
                                                                    format="dd/MM/yyyy"
                                                                    ng-model="created_at_fim.opt"
                                                                    ng-model-options='{ debounce: 500 }'
                                                                    placeholder=""
                                                                    date-input=""
                                                                    name=""
                                                                    is-open="data.open" 
                                                                    datepicker-options="dateOptions" 
                                                                    ng-click="data.open=true"
                                                                    ng-required="true" current-text="Hoje" 
                                                                    clear-text="Limpar"
                                                                    close-text="Close"
                                                                    current-text='Hoje' 
                                                                    alt-input-formats="altInputFormats" 
                                                                    ng-change="tndmnt_clnt_slctcs_lst_cntrllr.checaFiltrosCreatedAt()" />
                                                                    <span class="input-group-btn">
                                                                    <button type="button" class="btn btn-default" ng-click="data.open=true"><i class="glyphicon glyphicon-calendar"></i></button>
                                                                    </span>
                                                                    </p>
                                                                </div>
                                                                <div class="col-sm-1">
                                                         <td width="5%" class="text-left">
                                                            <a href ng-if="tndmnt_clnt_slctcs_lst_cntrllr.filtros.created_at_ini.length > 0 && tndmnt_clnt_slctcs_lst_cntrllr.filtros.created_at_fim.length > 0" ng-click="tndmnt_clnt_slctcs_lst_cntrllr.removeFiltroCreated_at(created_at.label);" class="btn btn-danger btn-small btns-acoes"><i class="el el-remove"></i></a>
                                                        </td>
                                                        </tr>
                                                        </div>
                                                                </td>
                                                             </td>
                                                        </tbody>
                                                        </div>
                                                        </div>

