                                                     <tbody ng-if="tndmnt_clnt_slctcs_lst_cntrllr.filtros.atribuidoa.length > 0 && !tndmnt_clnt_slctcs_lst_cntrllr.mim">

                                                        <tr>
                                                            <td class="td-title" colspan="2">
                                                                <b>Criado por:</b>
                                                            </td>
                                                        </tr>

