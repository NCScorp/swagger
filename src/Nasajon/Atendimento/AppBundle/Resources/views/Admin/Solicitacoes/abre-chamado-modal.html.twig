<form name="formAbreChamadoModal" novalidate="novalidate" role="form"  ng-submit="tndmnt_dmn_slctcs_lst_cntrllr.abrirChamadoProtocolo(tndmnt_dmn_slctcs_lst_cntrllr.numProtocolo)">
    <div class="modal-sm">
        <div class="modal-body" id="modal-body">
            <strong>NÃºmero do protocolo:</strong>
            <input ng-model="tndmnt_dmn_slctcs_lst_cntrllr.numProtocolo" autofocus type="text" class="form-control" onkeypress="return event.charCode >= 48 && event.charCode <= 57 || event.charCode == 13">
