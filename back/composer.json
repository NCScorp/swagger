{
    "name": "nasajon/symfony-skeleton-project",
    "license": "MIT",
    "type": "project",
    "description": "Projeto",
    "autoload": {
        "psr-4": {
            "": "src/",
            "Nasajon\\MDABundle\\": "src/Nasajon/MDABundle"
        },
        "classmap": ["app/AppKernel.php", "app/AppCache.php"]
    },
    "require": {
        "php": "7.1",
        "symfony/symfony": "^3.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "jms/serializer-bundle": "^2.0",
        "friendsofsymfony/rest-bundle": "^2.3",
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "firebase/php-jwt": "^4.0",
        "exercise/htmlpurifier-bundle": "^0.2.2",
        "nasajon/sdk": "dev-master",
        "nasajon/login-bundle": "dev-master",
        "sentry/sentry-symfony": "^2.0",
        "lightsaml/sp-bundle": "^1.1",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "knplabs/knp-gaufrette-bundle": "^0.6.1",
        "oneup/uploader-bundle": "^2.1",
        "aws/aws-sdk-php-symfony": "^2.0",
        "ekreative/health-check-bundle": "^1.4",
        "stevenmaguire/oauth2-keycloak": "^2.1",
        "guzzlehttp/guzzle": "^6.0",
        "maxbanton/cwh": "^1.1"
    },
    "require-dev": {
        "sensio/distribution-bundle": "^5.0.19",
        "nasajon/mdatransformer": "dev-master",
        "codeception/codeception": "^2.3.7",
        "nasajon/bancosweb": "dev-prod"
    },
    "config": {
        "platform": {
            "php": "7.1",
            "ext-bcmath": "1",
            "ext-sockets": "1"
        },
        "process-timeout": 3600
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "app",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Nasajon/LoginBundle"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/SDK"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/MdaPhpTransformer"
        },
        {
          "type": "git",
          "url": "https://github.com/Nasajon/BancosWeb"
        }
    ]
}